<Window x:Class="Nikpc.Windows.InvoicePrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nikpc.Windows"
        mc:Ignorable="d"
        Title="Számlák nyomtatása" Height="400" Width="300"
        Background="#FFB2D3CD">
    <Grid>
        <Label x:Name="label" Content="Számlák nyomtatása" HorizontalAlignment="Center" Height="43" Margin="0,20,0,0" VerticalAlignment="Top" Width="202" FontSize="20" FontWeight="Bold"/>
        <Label x:Name="label1" Content="Számlák:" HorizontalAlignment="Left" Height="26" Margin="10,63,0,0" VerticalAlignment="Top" Width="59"/>
        <ListBox x:Name="invoicesList" HorizontalAlignment="Left" Height="215" Margin="10,94,0,0" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="newInvoiceButton" Content="Új példány nyomtatása" HorizontalAlignment="Center" Height="40" Margin="0,321,0,0" VerticalAlignment="Top" Width="137"/>

    </Grid>
</Window>
